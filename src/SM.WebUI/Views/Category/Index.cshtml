@model List<Category>


<div class="container">
    <div class="row pt-4 pb-3">
        <div class=" col-6">
            <h2 class="text-primary">
                    Category list
            </h2>
        </div>
        <div class="col-6 text-end"> 
            <a asp-asp-controller="" asp-action="" class="btn btn-primary">
                <i class="bi bi-plus-circle"></i>   Create New Category
            </a>
        </div>
    </div>
</div>

<table class="table table-bordered table-striped">
    <thead>
    <tr>
        <th>
            Category name
        </th>
        <th>
            Display Id
        </th>
    </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td>@obj.Name</td>
                <td>@obj.Id</td>
            </tr>
        }
    </tbody>
</table>
